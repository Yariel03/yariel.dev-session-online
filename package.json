{
  "name": "yariel.dev-session-online",
  "version": "1.0.1",
  "description": "Libreria para saber cuantos usuarios estan en linea, se puede usar socket io para tiempo real o solo con peticiones",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:web && npm run build:node",
    "build:lib": "tsc",
    "build:web": "tsc  --module ES6 --outDir dist",
    "build:node": "tsc  --outDir dist-node",
    "start": "node dist/index.js",
    "publish:login": "npm login",
    "publish:public:": "npm publish --access public",
    "tsc:init": "tsc --init"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yariel03/yariel.dev-sessionOnline.git"
  },
  "keywords": [
    "yariel",
    "session",
    "sesion",
    "realtime",
    "activo",
    "online"
  ],
  "author": "Yariel Baldeon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Yariel03/yariel.dev-sessionOnline/issues"
  },
  "homepage": "https://github.com/Yariel03/yariel.dev-sessionOnline#readme",
  "devDependencies": {
    "typescript": "4.9.5"
  },
  "dependencies": {
    "socket.io": "4.5.4"
  }
}
